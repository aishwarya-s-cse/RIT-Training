import java.util.*;
 public class AlterLoop {
     public static void main(String[] args){
         System.out.println("The alternate numbers starting from 1 to 99 are:\n");
	       for (int i = 1; i < 100; i++) {
		 	         if (i % 2 != 0) {
		  	 			 System.out.println(i);
		     }     
		 }       
 }          
}          
